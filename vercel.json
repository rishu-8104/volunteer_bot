{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/slack/events",
      "dest": "/api/index.js"
    },
    {
      "src": "/slack/interactive",
      "dest": "/api/index.js"
    },
    {
      "src": "/slack/commands",
      "dest": "/api/index.js"
    }
  ]
}
